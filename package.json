{
  "name": "tminus",
  "version": "0.0.1",
  "private": true,
  "description": "Cloudflare-native temporal and relational intelligence engine",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "test:unit": "pnpm -r run test:unit",
    "test:integration": "pnpm -r run test:integration",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck",
    "deploy": "pnpm -r --filter './workers/*' run deploy"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.29.2",
  "devDependencies": {
    "smol-toml": "^1.6.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "workerd"
    ]
  }
}
